    mongodbStartValue++;

    mongodbValue.textContent = `${mongodbStartValue}%`;
    mongodbProgress.style.background = `conic-gradient(#ff0000 ${
        mongodbStartValue * 3.6
    }deg, #ededed 0deg)`;

    if (mongodbStartValue == mongodbEndValue) {
        clearInterval(progressmongodb);
    }
},mongodbspeed);
